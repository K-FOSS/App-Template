{
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [":8085"],
          "automatic_https": {
            "disable": true
          },
          "routes": [
            {
              "match": [
                {
                  "path": ["/graphql"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "API:8080"
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "path": ["/graphql"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "API:8080"
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "header": {
                    "Sec-WebSocket-Version": ["13"]
                  }
                },
                {
                  "path_regexp": {
                    "pattern": ".*js"
                  }
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "UI:8123"
                    }
                  ]
                }
              ]
            },
            {
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "UI:8085"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}
