{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "sourceType": "module",
  "targets": {
    "public": {
      "context": "browser",
      "includeNodeModules": true,
      "outputFormat": "esmodule"
    }
  },
  "scripts": {
    "test": "jest --verbose",
    "dev:hmr": "nodemon",
    "test:build": "ts-node --project bin/tsconfig.json ./bin/builder",
    "dev:debug": "node --nolazy --inspect-brk=0.0.0.0 --experimental-loader=@k-foss/ts-esnode --experimental-modules --experimental-specifier-resolution=node --harmony-top-level-await --harmony-optional-chaining ./Server/index.ts",
    "dev:ui": "parcel watch --no-cache UI/Client.tsx",
    "build:web": "parcel build UI/Client.tsx"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.37",
    "@apollo/react-hooks": "^4.0.0-beta.1",
    "@apollo/react-ssr": "^4.0.0-beta.1",
    "@authenio/samlify-node-xmllint": "^1.0.2",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "fastify": "^2.12.0",
    "fastify-cookie": "^3.6.0",
    "fastify-formbody": "^3.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "saml2-js": "^2.0.5",
    "samlify": "^2.7.1",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/fragment-matcher": "^1.12.2",
    "@k-foss/ts-esnode": "^1.1.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-nightly.127",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "nodemon": "^2.0.2",
    "parcel": "^2.0.0-nightly.125",
    "readable-stream": "^2.3.7",
    "ts-node": "^8.6.2"
  },
  "license": "ISC"
}
