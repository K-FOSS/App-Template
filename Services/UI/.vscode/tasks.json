{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "parcel-watch",
      "command": "npm",
      "args": ["run", "watch:web"],
      "type": "process",
      "isBackground": true,
      "problemMatcher": {
        "background": {
          "activeOnStart": true
        },
        "pattern": [
          {
            "regexp": "^([^\\\\s].*)\\\\((\\\\d+,\\\\d+)\\\\):\\\\s*(.*)$",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ]
      },
      "presentation": {
        "reveal": "never"
      }
    }
  ]
}
